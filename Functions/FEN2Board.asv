function out = FEN2Board(FENstr)
    out = [];
    row = 8;
    col = 1;
    % rnbqkbnr/pppppppp/8/8/8/3P4/PPP1PPPP/RNBQKBNR

    i = 1;
    while FENstr(i) ~= ' '
        temp = FENstr(i);
        if temp == '/'
            row = row - 1;
            
        else 
            try 
                spaces = int(temp);
                for j = 1:spaces
                    out(row,col) = 'e';
                    col = col+1


end